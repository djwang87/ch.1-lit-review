@article{yasudaChangesFlipperBeat2009,
  title = {Changes in Flipper Beat Frequency, Body Angle and Swimming Speed of Female Green Turtles {{{\emph{Chelonia}}}}{\emph{ Mydas}}},
  author = {Yasuda, T and Arai, N},
