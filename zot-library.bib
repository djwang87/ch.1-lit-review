@article{dealmeidajacomoHomeRangeSpatial2009,
  title = {Home {{Range}} and {{Spatial Organization}} of {{Maned Wolves}} in the {{Brazilian Grasslands}}},
  author = {de Almeida Jácomo, Anah Tereza and Kashivakura, Cyntia Kayo and Ferro, Claudia and Furtado, Mariana Malzoni and Astete, Samuel Perez and Tôrres, Natália Mundim and Sollmann, Rahel and Silveira, Leandro},
