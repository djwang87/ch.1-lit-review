@article{youngWildlifeGoingDogs2011,
  title = {Is {{Wildlife Going}} to the {{Dogs}}? {{Impacts}} of {{Feral}} and {{Free}}-Roaming {{Dogs}} on {{Wildlife Populations}}},
  shorttitle = {Is {{Wildlife Going}} to the {{Dogs}}?},
  author = {Young, Julie K. and Olson, Kirk A. and Reading, Richard P. and Amgalanbaatar, Sukh and Berger, Joel},
