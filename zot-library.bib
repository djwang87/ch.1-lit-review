@incollection{guinnessEvaluatingHowGroup2019,
  title = {Evaluating {{How}} the {{Group Size}} of {{Domestic}}, {{Invasive Dogs Affect Coastal Wildlife Responses}}: {{The Case}} of {{Flight}}-{{Initiation Distance}} ({{FID}}) of {{Birds}} on {{Southern Australian Beaches}}},
