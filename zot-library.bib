@article{ogadaEffectsVultureDeclines2012,
  title = {Effects of {{Vulture Declines}} on {{Facultative Scavengers}} and {{Potential Implications}} for {{Mammalian Disease Transmission}}},
  shorttitle = {Effects of {{Vulture Declines}} on {{Facultative Scavengers}} and {{Potential Implications}} for {{Mammalian Disease Transmission}}},
