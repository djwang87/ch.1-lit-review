@article{valentaBarkingRightTree2016,
  title = {Barking up the Right Tree: {{Understanding}} Local Attitudes towards Dogs in Villages Surrounding {{Ranomafana National Park}}, {{Madagascar}} Can Benefit Applied Conservation},
  shorttitle = {Barking up the Right Tree},
  author = {Valenta, Kim and Gettinger-Larson, Joseph A and Chapman, Colin A and Farris, Zachary J},
  date = {2016-12-23},
  journaltitle = {Madagascar Conservation \& Development},
  shortjournal = {MCD},
  volume = {11},
