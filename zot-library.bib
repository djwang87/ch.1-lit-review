@article{dossantosDogInvasionAgroforests2017,
  title = {Dog Invasion in Agroforests: {{The}} Importance of Households, Roads and Dog Population Size in the Surroundings},
  shorttitle = {Dog Invasion in Agroforests},
  author = {dos Santos, Cláudia Lilian Alves and Silva, Ana Paula and dos Santos, Sirleide Batista and Pardini, Renata and Cassano, Camila Righetto},
