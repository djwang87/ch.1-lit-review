  author = {Fossette, S. and Gleiss, A. C. and Myers, A. E. and Garner, S. and Liebsch, N. and Whitney, N. M. and Hays, G. C. and Wilson, R. P. and Lutcavage, M. E.},
  date = {2010-12-01},
  journaltitle = {Journal of Experimental Biology},
  shortjournal = {Journal of Experimental Biology},
