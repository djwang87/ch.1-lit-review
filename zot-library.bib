@article{childsDensityEstimatesRural1998,
  title = {Density Estimates of Rural Dog Populations and an Assessment of Marking Methods during a Rabies Vaccination Campaign in the {{Philippines}}},
  author = {Childs, James E and Robinson, Laura E and Sadek, Ramses and Madden, Anthony and Miranda, Mary Elizabeth and Miranda, Noel L},
