@article{yanDogsDiseaseThreats2020,
  title = {Dogs and {{Disease Threats}} to {{Giant Pandas}} in {{China}}},
  author = {Yan, Xia and Owens, Jacob R. and Wen, Yiping and Su, Xiaoyan and Wang, Zhenghao and Liu, Songrui and Zhang, Dongsheng and Callan, Ramana and Wenlei, Bi and Qi, Dunwu and Spotila, James R. and Hou, Rong and Zhang, Zhihe},
  date = {2020},
  journaltitle = {The Journal of Wildlife Management},
  volume = {84},
