@article{palUrineMarkingFreeranging2003,
  title = {Urine Marking by Free-Ranging Dogs ({{{\emph{Canis}}}}{\emph{ Familiaris}}) in Relation to Sex, Season, Place and Posture},
  author = {Pal, S.K},
  date = {2003-01},
  journaltitle = {Applied Animal Behaviour Science},
