@article{dohertyGlobalImpactsDomestic2017,
  title = {The Global Impacts of Domestic Dogs on Threatened Vertebrates},
  author = {Doherty, Tim S. and Dickman, Chris R. and Glen, Alistair S. and Newsome, Thomas M. and Nimmo, Dale G. and Ritchie, Euan G. and Vanak, Abi T. and Wirsing, Aaron J.},
  date = {2017-06},
