@article{homeCanineConundrumDomestic2018,
  title = {Canine {{Conundrum}}: Domestic Dogs as an Invasive Species and Their Impacts on Wildlife in {{India}}},
  shorttitle = {Canine {{Conundrum}}},
  author = {Home, C. and Bhatnagar, Y. V. and Vanak, A. T.},
  date = {2018-08},
  journaltitle = {Animal Conservation},
