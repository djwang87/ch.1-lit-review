@article{martinFieldObservationsAgonism2007,
  title = {Field {{Observations}} of {{Agonism}} in the {{Crayfish}}, {{Orconectes}} Rusticus: {{Shelter Use}} in a {{Natural Environment}}},
  shorttitle = {Field {{Observations}} of {{Agonism}} in the {{Crayfish}}, {{Orconectes}} Rusticus},
