  shorttitle = {How {{Does}} a {{Carnivore Guild Utilise}} a {{Substantial}} but {{Unpredictable Anthropogenic Food Source}}?},
  author = {Forsyth, David M. and Woodford, Luke and Moloney, Paul D. and Hampton, Jordan O. and Woolnough, Andrew P. and Tucker, Mark},
  editor = {Boyce, Mark S.},
  date = {2014-06-11},
  journaltitle = {PLoS ONE},
  shortjournal = {PLoS ONE},
  volume = {9},
