@article{cardozoComparationLogisticRegression2010,
  title = {Comparation of Logistic Regression Methods and Discrete Choice Model in the Selection of Habitats},
  author = {Cardozo, Sandra Vergara and Manly, Bryan Frederick John and Dias, Carlos Tadeu dos Santos},
