issn = {0022-0949, 1477-9145},
journal = {The Journal of Experimental Biology},
keywords = {accelerometer,body density,boyle,cost of swimming,endotherm,gas solubility,s law},
number = {23},
pages = {4074--4083},
pmid = {21075949},
title = {{Behaviour and buoyancy regulation in the deepest-diving reptile: the leatherback turtle}},
url = {http://jeb.biologists.org/content/213/23/4074{\%}5Cnhttp://jeb.biologists.org/content/213/23/4074.full.pdf{\%}5Cnhttp://jeb.biologists.org/content/213/23/4074.short{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/21075949},
