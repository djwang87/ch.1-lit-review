@article{Ruiz-Izaguirre2015,
abstract = {Village dogs are reported to prey on sea-turtle nests at various beaches$\backslash$nworldwide. Sea-turtle species present in Mexico include six species,$\backslash$nwhich are listed under the International Union for Conservation of$\backslash$nNature Red List. It is however not clear why dogs scavenge and how they$\backslash$nenter nesting areas; this hinders effective management of dogs at$\backslash$nsea-turtle nesting beaches. Hunger, for example, could be a driving$\backslash$nfactor for village dogs to scavenge sea-turtle nests. The aim of this$\backslash$nstudy was to gain insight into roaming characteristics and feeding$\backslash$npractices of dogs scavenging sea-turtle nests. Movements of 19 village$\backslash$ndogs (9 nest scavengers and 10 non-nest scavengers) at Colola village$\backslash$nand beach (an important nesting ground for the eastern Pacific green$\backslash$nturtle: Chelonia mydas) were monitored through radio-tracking and direct$\backslash$nobservations. A prerequisite for nest scavenging is traveling to the$\backslash$nbeach. We computed distance from home to beach' and the activity range'$\backslash$nof nest scavengers and non-nest scavengers. Furthermore, the dogs'$\backslash$nowners were interviewed regarding feeding practices. Nest scavengers had$\backslash$na lower metabolic energy intake of their daily food corn tortillas$\backslash$n(296kJkg(-1) BW0.75) than non-nest scavengers (464kJkg(-1) BW0.75)$\backslash$n(two-sample t-test=2.67, P=0.017). Moreover, 39{\%} of owners reported$\backslash$nthat they provided turtle eggs or egg shells to their dogs at least$\backslash$nonce. The activity range of nest scavengers was significantly larger$\backslash$nthan that of non-nest scavengers (F=11.64, P=0.007). Dogs were generally$\backslash$nfound at the beach at night (42{\%}) and dawn (34{\%}). Our findings have$\backslash$nimplications for the management of dogs at sea-turtle nesting beaches.$\backslash$nWe recommend that dogs' movements should be restricted between night$\backslash$n(09:00pm) and dawn (06:00am) and that sufficient and adequate feeding of$\backslash$ndogs should be promoted among dog owners.},
author = {Ruiz-Izaguirre, E. and van Woersem, A. and Eilers, K (C.) H. A. M. and van Wieren, S. E. and Bosch, G. and van der Zijpp, A. J. and de Boer, I J M},
