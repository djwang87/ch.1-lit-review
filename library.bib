keywords = {Behaviour,Desert locust,Distribution patterns,Gregarization,Host plants,Phase change,Resources,Schistocerca gregaria},
number = {8},
pages = {949--957},
pmid = {20493192},
publisher = {Elsevier Ltd},
title = {{Phase polyphenism and preventative locust management}},
