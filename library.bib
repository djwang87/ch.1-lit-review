file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Atickem, Bekele, Williams - 2010 - Competition between domestic dogs and Ethiopian wolf (iCanis simensisi) in the Bale Mountains Nationa.pdf:pdf},
isbn = {0141-6707},
