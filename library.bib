keywords = {GPS,LoCoH,Movement ecology,Movement phase,Revisitation,Springbok,T-LoCoH,Time use,Utilization distribution,gps,locoh,movement ecology,movement phase,revisitation,springbok,t-locoh,time use,utilization distribution},
number = {1},
pages = {1--14},
pmid = {25709816},
