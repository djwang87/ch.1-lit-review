isbn = {0952836905007},
issn = {0952-8369},
journal = {The Zoological Society of London},
keywords = {giant anteater,habitat use,home range,myrmecophaga tridactyla,myrmecophagidae,pantanal,radio-telemetry,wetland},
number = {04},
pages = {365--375},
title = {{Home range of giant anteaters (Myrmecophaga tridactyla) in the Pantanal wetland, Brazil}},
