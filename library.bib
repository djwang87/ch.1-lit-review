arxivId = {1726-4189/bg/2005-2-1},
author = {Nagelkerken, I. and Blaber, S. J M and Bouillon, S. and Green, P. and Haywood, M. and Kirton, L. G. and Meynecke, J. O. and Pawlik, J. and Penrose, H. M. and Sasekumar, A. and Somerfield, P. J.},
doi = {10.1016/j.aquabot.2007.12.007},
eprint = {bg/2005-2-1},
file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nagelkerken et al. - 2008 - The habitat function of mangroves for terrestrial and marine fauna A review.pdf:pdf},
