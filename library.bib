file = {:C$\backslash$:/Users/djwem/OneDrive - Nanyang Technological University/Journals/mosser2014.pdf:pdf},
issn = {13652656},
journal = {Journal of Animal Ecology},
keywords = {Accelerometry,Biotelemetry,Energetics,Landscape,Rangifer},
number = {4},
pages = {916--922},
title = {{Towards an energetic landscape: Broad-scale accelerometry in woodland caribou}},
volume = {83},
year = {2014}
}
