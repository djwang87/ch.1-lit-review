title = {{Dietary niche overlap of free-roaming dingoes and domestic dogs: the role of human-provided food}},
url = {http://www.bioone.org/doi/abs/10.1644/13-MAMM-A-145.1 http://www.bioone.org/doi/abs/10.1644/13-MAMM-A-145.1{\%}5Cnhttp://www.asmjournals.org/doi/abs/10.1644/13-MAMM-A-145.1},
