journal = {American Journal of Veterinary Research},
number = {11},
pages = {1781--1786},
pmid = {23106464},
title = {{Plasma 25-hydroxyvitamin D3 concentrations in Hermann's tortoises ({\textless}i{\textgreater}Testudo hermanni{\textless}/i{\textgreater}) exposed to natural sunlight and two artificial ultraviolet radiation sources}},
