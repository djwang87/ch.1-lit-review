isbn = {9785919780250},
issn = {19326203},
journal = {PLoS ONE},
number = {5},
pages = {1--13},
title = {{Potential of the desert locust {\textless}i{\textgreater}Schistocerca gregaria{\textless}/i{\textgreater} (Orthoptera: Acrididae) as an unconventional source of dietary and therapeutic sterols}},
volume = {10},
year = {2015}
