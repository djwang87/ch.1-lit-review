keywords = {Acceleration,Acinonyx,Alligators and Crocodiles,Animal,Animals,Behavior,Dogs,Echidna,Endangered Species,Macropodidae,Mustelidae,Running,Support Vector Machine,Telemetry,Telemetry: methods,Walking},
