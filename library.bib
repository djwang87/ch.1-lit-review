file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wilson, Wolkovich - 2011 - Scavenging How carnivores and carrion structure communities.pdf:pdf},
isbn = {0169-5347 (Print)$\backslash$n0169-5347 (Linking)},
