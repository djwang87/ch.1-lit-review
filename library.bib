journal = {Zoo Biol.},
keywords = {Attenuation,UVB,Vitamin D},
number = {5},
pages = {417--423},
pmid = {19360590},
title = {{Evaluation of UVB reduction by materials commonly used in reptile husbandry}},
volume = {26},
year = {2007}
