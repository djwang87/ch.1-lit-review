file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rothman et al. - 2014 - Nutritional contributions of insects to primate diets Implications for primate evolution.pdf:pdf},
isbn = {0047-2484},
