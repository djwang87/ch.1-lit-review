issn = {0040-3709},
journal = {Teratology},
keywords = {Abnormalities, Drug-Induced,Abnormalities, Drug-Induced: epidemiology,Cohort Studies,Dose-Response Relationship, Drug,Female,Humans,Pregnancy,Pregnancy Trimester, First,Prevalence,Prospective Studies,Vitamin A,Vitamin A: administration {\&} dosage,Vitamin A: adverse effects},
