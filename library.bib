journal = {Applied Animal Behaviour Science},
keywords = {corticosterone,handling,reptile,stress,welfare},
number = {3-4},
pages = {323--336},
title = {{Physiological and behavioral effects of handling and restraint in the ball python (Python regius) and the blue-tongued skink (Tiliqua scincoides)}},
