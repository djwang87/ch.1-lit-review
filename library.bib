journal = {Proceedings of the Royal Society B: Biological Sciences},
month = {feb},
number = {1609},
pages = {577--582},
pmid = {17476779},
publisher = {The Royal Society},
title = {{Tameness and stress physiology in a predator-naive island species confronted with novel predation threat}},
