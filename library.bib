isbn = {2051-3933 (Electronic)$\backslash$r2051-3933 (Linking)},
issn = {2051-3933},
journal = {Movement Ecology},
keywords = {animal movement,computational geometry,geographic information science,interdisciplinary developments,movement ecology,spatio-temporal,spatio-temporal analysis,trajectories,visual analytics,visualisation},
number = {1},
pages = {5},
pmid = {25874114},
title = {{Analysis and visualisation of movement: an interdisciplinary review}},
