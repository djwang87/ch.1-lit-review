file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Resheff et al. - 2015 - Topic Modeling of Behavioral Modes Using Sensor Data(2).pdf:pdf},
issn = {2364-415X},
journal = {International Journal of Data Science and Analytics},
number = {1},
