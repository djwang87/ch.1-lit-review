file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Torres, Prado - 2010 - Domestic dogs in a fragmented landscape in the Brazilian Atlantic Forest abundance, habitat use and caring by (2).pdf:pdf},
