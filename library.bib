issn = {25300644},
journal = {Perspectives in Ecology and Conservation},
keywords = {Canis lupus familiaris,Exotic species,Invasion ecology,Mammals},
number = {4},
pages = {228--233},
publisher = {Associa{\c{c}}{\~{a}}o Brasileira de Ci{\^{e}}ncia Ecol{\'{o}}gica e Conserva{\c{c}}{\~{a}}o},
title = {{Who let the dogs out? Occurrence, population size and daily activity of domestic dogs in an urban Atlantic Forest reserve}},
