file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Walter, Onorato, Fischer - 2015 - Is there a single best estimator Selection of home range estimators using area-under-the-curve.pdf:pdf},
isbn = {0021-8790 (Print)$\backslash$r0021-8790 (Linking)},
issn = {2051-3933},
journal = {Movement Ecology},
keywords = {Area-under-the-curve,Brownian bridge movement mode,area-under-the-curve,brownian bridge movement models,estimators,global positioning systems,home range,isopleth,kernel density,schedule},
number = {1},
