journal = {Journal of applied animal welfare science : JAAWS},
number = {2},
pages = {85--109},
pmid = {16956315},
title = {{Environmental enrichment alters the behavioral profile of ratsnakes (Elaphe).}},
volume = {9},
year = {2006}
}
