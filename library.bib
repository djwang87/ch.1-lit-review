@article{Loock2018,
abstract = {As the environment becomes increasingly altered by human development, the importance of understanding the ways in which wildlife interact with modified landscapes is becoming clear. Areas such as industrial sites are sometimes presumed to have little conservation value, but many of these sites have areas of less disturbed habitats around their core infrastructure, which could provide ideal conditions to support some species, such as mesocarnivores. We conducted the first assessments of the density of serval (Leptailurus serval) at the Secunda Synfuels Operations plant, South Africa, using camera trap surveys analysed within a spatially explicit capture recapture framework. We show that servals occurred at densities of 76.20–101.21 animals per 100 km², which are higher than previously recorded densities for this species, presumably due to high abundance of prey and the absence of persecution and/or competitor species. Our findings highlight the significant conservation potential of industrialised sites, and we suggest that such sites could help contribute towards meeting conservation goals.},
author = {Loock, Daan J.E. and Williams, Samual T. and Emslie, Kevin W. and Matthews, Wayne S. and Swanepoel, Lourens H.},
doi = {10.1038/s41598-018-34936-0},
file = {:C$\backslash$:/Users/djwem/OneDrive - Nanyang Technological University/Journals/s41598-018-34936-0.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
number = {1},
pages = {1--9},
title = {{High carnivore population density highlights the conservation value of industrialised sites}},
volume = {8},
year = {2018}
}
@article{Simcharoen2014,
abstract = {Prey density is declining throughout the tiger's (Panthera tigris) range$\backslash$nand knowledge of the ecological factors that affect prey distribution$\backslash$nand abundance remains surprisingly limited for this globally endangered$\backslash$nspecies. In this study, we examined the ecological variables influencing$\backslash$nthe abundance of sambar (Rusa unicolor), the dominant prey species for$\backslash$nthe tiger across its global southern range. We also identified the scale$\backslash$nat which these variables impact sambar distribution in Huai Kha Khaeng$\backslash$nWildlife Sanctuary, a high tiger density site in Southeast Asia. The$\backslash$nfecal pellet group accumulation method was used to estimate an index of$\backslash$nsambar abundance. Pellet groups were counted along 360 line transects$\backslash$nrandomly placed among four approximately 100 km(2) sites that$\backslash$nencompassed six female tiger home ranges. The relationship between$\backslash$nsambar pellet-group counts and 10 environmental variables was$\backslash$ninvestigated using generalised linear mixed models. The sambar abundance$\backslash$nindex was negatively associated with distance to the largest river in$\backslash$nthe study area, elevation, and the amount of dry deciduous dipterocarp$\backslash$nforest cover. Distribution and abundance of sambar were positively$\backslash$nassociated with relatively flat areas of river valleys, presumably due$\backslash$nto the quality of vegetation available for foraging and greater$\backslash$nvisibility for detecting predators compared to other portions of the$\backslash$nstudy area. This study is the first to identify the importance of wide$\backslash$nalluvial valleys to tiger prey and suggests this habitat is critical for$\backslash$nsecuring one of the largest tiger source populations in Southeast Asia.},
author = {Simcharoen, Achara and Savini, Tommaso and Gale, George A. and Roche, Erin and Chimchome, Vijak and Smith, James L D},
file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Simcharoen et al. - 2014 - Ecological factors that influence sambar (iRusa unicolori) distribution and abundance in western Thailand Imp.pdf:pdf},
issn = {02172445},
journal = {Raffles Bulletin of Zoology},
keywords = {Habitat selection,Huai Kha Khaeng Wildlife Sanctuary,Panthera tigris corbetti,Tiger prey},
number = {March},
pages = {100--106},
title = {{Ecological factors that influence sambar ({\textless}i{\textgreater}Rusa unicolor{\textless}/i{\textgreater}) distribution and abundance in western Thailand: Implications for tiger conservation}},
volume = {62},
year = {2014}
}
