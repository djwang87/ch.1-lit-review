doi = {10.1371/journal.pone.0168176},
file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Morato et al. - 2016 - Space use and movement of a neotropical top predator The endangered jaguar.pdf:pdf},
isbn = {2014249210},
