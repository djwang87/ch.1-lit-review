@article{Farr1974,
abstract = {Various controlled densities of guppies, Poecilia reticulata, wer observed in order to determine the effects of population density on frequency of courtship and agonistic interactions. Populations with single male exhibit a low level of courtship activity; the presence of a second male increases this level of activity markedly. Populations with a 1:1 sex ratio in densities greater than one pair exhibit a constant mean number of courtship interactions but, with increased crowding, an increasing variance in level of courtship activity. This results in a frequency profile of courtship interaction characteristic of each observed density. Each population density exhibits a density-dependent pattern of social interaction defined quantitatively by frequencies of courtship aggressive interactions at the population level. The contribution of the behaviour of individual males and to this patterning of social interaction in each population was partially determined.},
author = {Farr, James A. and Herrnkind, William F.},
doi = {10.1016/S0003-3472(74)80003-5},
issn = {00033472},
journal = {Animal Behaviour},
month = {aug},
number = {3},
pages = {582--591},
title = {{A quantitative analysis of social interaction of the guppy, Poecilia reticulata (pisces: Poeciliidae) as a function of population density}},
url = {http://www.sciencedirect.com/science/article/pii/S0003347274800035},
volume = {22},
year = {1974}
}
@article{Moon2000,
abstract = {Constriction of prey by gopher snakes Pituophis melanoleucus and king$\backslash$nsnakes Lampropeltis getula is$\backslash$n$\backslash$nhighly variable in posture, muscular activity and force exertion.$\backslash$nThese snakes typically use lateral bends of$\backslash$n$\backslash$nthe anterior trunk to wind the body into a vertical coil around the$\backslash$nprey. Three common constriction$\backslash$n$\backslash$npostures are fully encircling loops that form a coil, partially encircling$\backslash$nloops, and non-encircling loops that$\backslash$n$\backslash$npinion the prey. Initial tightening of a coil occurs by winding or$\backslash$npressing the loops tighter to reduce the$\backslash$n$\backslash$ndiameter of the coil. The epaxial muscles are highly active during$\backslash$nstriking and coil formation and$\backslash$n$\backslash$nintermittently active during sustained constriction. These results$\backslash$nrefute the hypothesis of a mechanical$\backslash$n$\backslash$nconstraint on constriction in snakes with elongate epaxial muscles.$\backslash$nConstricting gopher and king snakes$\backslash$n$\backslash$ncan detect muscular, ventilatory and circulatory movements in rodent$\backslash$nprey. In response to simulated$\backslash$n$\backslash$nheartbeats or ventilation in mice, the snakes twitch visibly, recruit$\backslash$nepaxial muscle activity, and increase$\backslash$n$\backslash$nconstriction pressure temporarily, but then quickly relax. Muscular$\backslash$nactivity and constriction pressure are$\backslash$n$\backslash$nincreased most and sustained longest in response to muscular struggling$\backslash$nin prey. Although muscle activity$\backslash$n$\backslash$nand pressure exertion are intermittent, the constriction posture is$\backslash$nmaintained until the prey has been$\backslash$n$\backslash$ncompletely still for several seconds; thus, a snake can reapply pressure$\backslash$nin response to any circulatory,$\backslash$n$\backslash$nventilatory or muscular movement by the prey. The pressures of 6.1±30.9$\backslash$nkPa (46±232 mm Hg) exerted on$\backslash$n$\backslash$nsmall mammal prey by constricting snakes range from about half to$\backslash$nover twice a mouse's systolic blood$\backslash$n$\backslash$npressure, and are probably 10 times larger than the venous pressure.$\backslash$nThese high pressures probably kill$\backslash$n$\backslash$nmammalian prey by inducing immediate circulatory and cardiac arrest,$\backslash$nrather than by suffocation alone.},
author = {Moon, Brad R.},
doi = {10.1017/S0952836900009109},
file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Moon - 2000 - The mechanics and muscular control of constriction in gopher snakes (Pituophis melanoleucus) and a king snake (Lampropelti.pdf:pdf},
issn = {09528369},
journal = {Journal of Zoology},
keywords = {Pituophis melanoleucus,biomechanics,constriction,electromyography,feeding,gopher snake,kinematics,king snake,lampropeltis getula,melanoleucus,muscle,pituophis,snakes},
number = {1},
pages = {83--98},
title = {{The mechanics and muscular control of constriction in gopher snakes (Pituophis melanoleucus) and a king snake (Lampropeltis getula)}},
