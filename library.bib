file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gallagher et al. - 2017 - Energy Landscapes and the Landscape of Fear.pdf:pdf},
issn = {01695347},
