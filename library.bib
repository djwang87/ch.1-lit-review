number = {4},
pages = {987--994},
pmid = {21180903},
title = {{Domestic dogs in a fragmented landscape in the Brazilian Atlantic Forest: abundance, habitat use and caring by owners.}},
volume = {70},
year = {2010}
}
@article{Gherardi2004e,
