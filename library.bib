journal = {Ethology},
pages = {289--302},
pmid = {5031},
title = {{An observational study of coyote ({\textless}i{\textgreater}Canis latrans{\textless}/i{\textgreater}) scent-marking and territoriality in Yellowstone National Park}},
volume = {105},
year = {1999}
}
