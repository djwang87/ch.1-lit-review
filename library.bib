isbn = {0962-1083},
issn = {0962-1083},
journal = {Molecular Ecology},
keywords = {Biogeography,Cichlids,Lake Malawi,Mbuna,Microsatellites,Migration},
number = {6},
pages = {1013--1026},
title = {{Biogeography and population genetics of the Lake Malawi cichlid Melanochromis auratus: Habitat transience, philopatry and speciation}},
volume = {8},
year = {1999}
