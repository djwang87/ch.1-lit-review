doi = {10.1080/00063657.2014.934193},
file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Broughton et al. - 2014 - Winter habitat selection by Marsh Tits Poecile palustris in a British woodland.pdf:pdf},
