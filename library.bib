isbn = {1477-9145 (Electronic)$\backslash$r0022-0949 (Linking)},
issn = {1477-9145},
journal = {The Journal of experimental biology},
keywords = {Acceleration,Acinonyx,Alligators and Crocodiles,Animal,Animals,Behavior,Dogs,Echidna,Endangered Species,Macropodidae,Mustelidae,Running,Support Vector Machine,Telemetry,Telemetry: methods,Walking,accelerometry,accepted 28 august 2013,biotelemetry,endangered species,movement ecology,received 16 april 2013,support vector machines,svms},
