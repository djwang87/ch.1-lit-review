file = {:C$\backslash$:/Users/djwem/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Montecino-Latorre, San Mart{\'{i}}n - 2018 - Evidence supporting that human-subsidized free-ranging dogs are the main cause of animal losses i.pdf:pdf},
